{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1",
      "phpConfig": {
        "error_reporting": "E_ALL & ~E_DEPRECATED & ~E_NOTICE",
        "display_errors": "Off",
        "log_errors": "On"
      }
    }
  },
  "routes": [
    {
      "src": "/(css|js|images|fonts)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
